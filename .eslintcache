ord,\n    //   EmpresaId: 101,\n    //   Navegador: '',\n    //   Dispositivo: '',\n    //   Ip: '',\n    //   Informacoes: null,\n    // });\n\n    const response = await api.get('/autenticacao');\n\n    const { IdUsuario, NomeUsuario } = response.data;\n\n    const user = {\n      id: IdUsuario.toString(),\n      name: NomeUsuario,\n    };\n\n    localStorage.setItem('@TelaAgil:user', JSON.stringify(user));\n\n    setData({ user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@TelaAgil:user');\n\n    setData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: data.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n","/home/roberto/Área de Trabalho/eqtl-tela-agil/src/services/api.ts",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","severity":1,"message":"104","line":33,"column":5,"nodeType":"105","messageId":"106","endLine":33,"endColumn":16},{"ruleId":"107","severity":1,"message":"108","line":38,"column":39,"nodeType":"109","messageId":"110","endLine":38,"endColumn":47},{"ruleId":"107","severity":1,"message":"111","line":38,"column":49,"nodeType":"109","messageId":"110","endLine":38,"endColumn":57},"lines-around-directive",["112"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpression","unexpected","@typescript-eslint/no-unused-vars","'username' is defined but never used.","Identifier","unusedVar","'password' is defined but never used.","padding-line-between-statements"]